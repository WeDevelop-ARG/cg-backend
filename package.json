{
  "name": "cg-backend",
  "version": "1.0.0",
  "description": "Backend for cg",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon -w src --config --ext js,graphql --exec \"babel-node src\"",
    "dev:with-migrations": "npx sequelize db:migrate && npm run dev",
    "build": "npm run cleanup && babel src -D -d dist",
    "start": "node dist/index.js",
    "cleanup": "rm -rf dist"
  },
  "keywords": [
    "Node.js",
    "GraphQL"
  ],
  "license": "ISC",
  "dependencies": {
    "amazon-cognito-identity-js": "^3.2.5",
    "apollo-server-express": "^2.3.1",
    "aws-sdk": "^2.660.0",
    "axios": "^0.19.2",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "graphql": "^14.1.1",
    "graphql-resolvers": "^0.4.2",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mercadopago": "^1.3.1",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@types/jest": "^25.2.1",
    "babel-jest": "^26.0.1",
    "babel-plugin-root-import": "^6.1.0",
    "jest": "^26.0.1",
    "nodemon": "^1.18.9",
    "standard": "^14.3.1"
  }
}
